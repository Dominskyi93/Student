import java.util.Scanner;
    public class Main {
        public static void main(String[] args) {
            Scanner scan = new Scanner(System.in);
            System.out.println("Введіть число: ");
            int count = scan.nextInt();
            int i = 1;
            int maxNov = i;
            while(i < count) {
            i++;
            }
            int max = Math.max(i, maxNov);
            System.out.println(max);
        }
    }
