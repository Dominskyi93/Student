public class Main {

    public static void main(String[] args) {
        System.out.println("Максимальное число это - " + getMaxInteger(12, 6, 13));
    }

    public static int getMaxInteger(int a, int b, int c) {
        int result = 0;
        if ((a > b) && (a > c)) {
            result = a;
        } else if ((b > a) && (b > c)) {
            result = b;
        } else {
            result = c;
        }
        return result;
    }
}
